
<% @users.each do |user| %>
      <tr>
        <td><%= user.name %></td>
        <td><%= user.lastname %></td>
        <td><%= user.username %></td>
        <td><%= user.email %></td>
       
        <td><i class="far fa-edit"></i><%= link_to 'Edit', edit_user_registration_path(user), remote: true %>
      &nbsp <i class="fas fa-user-times"></i> <%= link_to 'Delete', user_registration_path(user), method: :delete, data: { confirm: 'Are you sure?' } %></td>
        
      </tr>
    <% end %>